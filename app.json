{
  "expo": {
    "name": "un-app",
    "slug": "unapp",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      ["expo-location", {"locationWhenInUsePermission": "un-app uses your location to show nearby restaurants and calculate cab fares."}],
      "expo-calendar"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ai.unapp.mobile",
      "buildNumber": "7",
      "infoPlist": {
        "NSCameraUsageDescription": "un-app does not use your camera",
        "NSCalendarsUsageDescription": "un-app learns WHEN you're busy to predict your needs better",
        "NSCalendarsFullAccessUsageDescription": "un-app reads your calendar to find free gaps and busy patterns â€” no event details are stored",
        "LSApplicationQueriesSchemes": [
          "swiggy",
          "zomato",
          "googlecalendar",
          "nammayatri",
          "uber",
          "olacabs",
          "rapido"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "ai.unapp.mobile"
    },
    "extra": {
      "eas": {
        "projectId": "23f6341c-8e6d-4fe3-b61b-9f753d508143"
      }
    },
    "owner": "swapnil_un-app"
  }
}
